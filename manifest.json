{
  "manifest_version": 3,
  "name": "YouTube Popup Player",
  "version": "1.0",
  "description": "Watch YouTube videos in a popup window.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.ico",
      "48": "images/icon48.ico",
      "128": "images/icon128.ico"
    }
  }
}
